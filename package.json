{
  "name": "animated-pixel-gradients",
  "version": "3.0.0",
  "description": "Make pixelated gradient GIFs with words",
  "main": "server.js",
  "scripts": {
    "build:prod": "webpack --config webpack.prod.js",
    "build:dev": "webpack --config webpack.dev.js",
    "serve": "node server.js",
    "start": "npm run build:prod && env NODE_ENV=production npm run serve",
    "start:dev": "npm run build:dev && env NODE_ENV=development npm run serve",
    "format": "prettier --write .",
    "lint": "eslint ."
  },
  "author": "Noelle Leigh",
  "license": "MIT",
  "dependencies": {
    "dotenv": "^10.0.0",
    "express": "^4.19.2",
    "gif.js": "^0.2.0",
    "mainloop.js": "^1.0.4",
    "opentype.js": "^1.3.4"
  },
  "devDependencies": {
    "css-loader": "^6.5.1",
    "eslint": "^8.2.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "html-webpack-plugin": "^5.5.0",
    "prettier": "2.4.1",
    "style-loader": "^3.3.1",
    "webpack": "^5.76.0",
    "webpack-cli": "^4.9.1",
    "webpack-merge": "^5.8.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/noelleleigh/animated-pixel-gradients.git"
  },
  "keywords": [
    "pixel-art",
    "gif"
  ],
  "bugs": {
    "url": "https://github.com/noelleleigh/animated-pixel-gradients/issues"
  },
  "homepage": "https://github.com/noelleleigh/animated-pixel-gradients#readme",
  "directories": {
    "test": "test"
  },
  "engines": {
    "node": "16"
  }
}
